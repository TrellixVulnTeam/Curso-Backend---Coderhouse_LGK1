<div class="container mt-3">
    {{#if productsExists}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;">
                <th>Title</th>
                <th>Price</th>
                <th>Thumbnail</th>
            </tr>
            {{#each productos}}
            <tr>
                <td>{{this.title}}</td>
                <td>${{this.price}}</td>
                <td><img width="40" , src={{this.thumbnail}}></td>
            </tr>
            {{/each}}
        </table>
    </div>
    {{else}}
    <h3 class="alert alert-danger">No se encontraron productos</h3>
    <br>
    {{/if}}
    <br>
    <a href="/" type="button" class="btn btn-danger mt-3">Volver</a>